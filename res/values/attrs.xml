<?xml version="1.0" encoding="utf-8"?>
<resources>
<declare-styleable name="Gallery">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>
    
    <declare-styleable name="StarRatingView">
        <attr name="star_on" format="reference" />
        <attr name="star_off" format="reference" />
        <attr name="rating" format="integer" />
        <attr name="rateable" format="boolean" />
    </declare-styleable>
    
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
    
</resources>